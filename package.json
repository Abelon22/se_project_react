{
  "name": "se_project_react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "server": "json-server --watch db.json --id _id --port 3001", 
    "dev:server":  "json-server --watch db.json --id _id --port 3001", 
    "dev:vite":   "wait-on http://localhost:3001 && vite --open", 
    "run:server-app": "concurrently -k \"npm:dev:server\" \"npm:dev:vite\""
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "json-server": "^0.17.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.3.0",
    "vite": "^5.3.1",
    "wait-on": "^8.0.5"
  },
  "homepage": "https://Abelon22.github.io/se_project_react/"
}
